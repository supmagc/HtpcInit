
function is_pid_valid_for_name {
  local PID="$1"
  local NAME="$2"
  if [ "$NAME" = "$(ps -e | grep $PID | awk '{print $4}')" ]; then
    echo "true"
  else
    echo "false"
  fi
}

function get_pid_from_file {
  local FILE="$1"
  local PID=0
  if [ -f "$FILE" ]; then
    PID=$(<"$FILE")
  fi
  echo "$PID"
}