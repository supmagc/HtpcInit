
function validate_pid_for_name {
  local PID="$1"
  local NAME="$2"
  local TEST=
  if [ $NAME == $(ps -e | grep $PID | awk '{print $4}') ]; then
    echo "running"
  fi
}

function get_pid_from_file {
  local FILE="$1"
  local PID=0
  if [ -f "$FILE" ]; then
    PID=$(<"$FILE")
  fi
  echo "$PID"
}